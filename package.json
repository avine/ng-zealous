{
  "name": "starter",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "serve:ssr:starter": "node dist/starter/server/server.mjs",
    "start:docs": "ng serve ng-zealous-docs",
    "build:docs": "ng build ng-zealous-docs",
    "build:github-pages": "ng build ng-zealous-docs -c production,github-pages",
    "postbuild:github-pages": "cp docs/index.html docs/404.html",
    "serve:ssr:docs": "node dist/ng-zealous-docs/server/server.mjs",
    "prebuild:lib": "pnpm --dir scripts/update-ng-zealous-peer-deps start",
    "build:lib": "ng build ng-zealous",
    "test:lib": "ng test ng-zealous",
    "e2e": "playwright test",
    "format": "prettier --write .",
    "lint": "ng lint --fix"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ],
    "plugins": [
      "prettier-plugin-organize-imports"
    ]
  },
  "private": true,
  "packageManager": "pnpm@10.24.0",
  "dependencies": {
    "@angular/aria": "^21.0.1",
    "@angular/cdk": "^21.0.1",
    "@angular/common": "^21.0.2",
    "@angular/compiler": "^21.0.2",
    "@angular/core": "^21.0.2",
    "@angular/forms": "^21.0.2",
    "@angular/material": "^21.0.1",
    "@angular/material-date-fns-adapter": "^21.0.1",
    "@angular/platform-browser": "^21.0.2",
    "@angular/platform-server": "^21.0.2",
    "@angular/router": "^21.0.2",
    "@angular/ssr": "^21.0.1",
    "@fontsource/nunito": "^5.2.7",
    "@fontsource/source-code-pro": "^5.2.7",
    "@highlightjs/cdn-assets": "^11.11.1",
    "@material-symbols/font-500": "^0.40.0",
    "axe-core": "^4.11.0",
    "date-fns": "^4.1.0",
    "express": "^5.2.1",
    "highlight.js": "^11.11.1",
    "marked": "^17.0.1",
    "rxjs": "~7.8.2",
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@angular-eslint/builder": "^21.0.1",
    "@angular/build": "^21.0.1",
    "@angular/cli": "^21.0.1",
    "@angular/compiler-cli": "^21.0.2",
    "@angular/localize": "^21.0.2",
    "@eslint/js": "^9.39.1",
    "@playwright/test": "^1.57.0",
    "@types/express": "^5.0.6",
    "@types/node": "^24.10.1",
    "angular-eslint": "^21.0.1",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "jsdom": "^27.2.0",
    "ng-packagr": "^21.0.0",
    "prettier": "^3.7.3",
    "prettier-plugin-organize-imports": "^4.3.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.48.1",
    "vitest": "^4.0.14"
  }
}
