{
  "name": "starter",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "serve:ssr:starter": "node dist/starter/server/server.mjs",
    "format": "prettier --write .",
    "serve:ssr:ng-zealous-docs": "node dist/ng-zealous-docs/server/server.mjs"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ],
    "plugins": [
      "prettier-plugin-organize-imports"
    ]
  },
  "private": true,
  "packageManager": "pnpm@10.20.0",
  "dependencies": {
    "@angular/cdk": "21.0.0",
    "@angular/common": "^21.0.0",
    "@angular/compiler": "^21.0.0",
    "@angular/core": "^21.0.0",
    "@angular/forms": "^21.0.0",
    "@angular/material": "21.0.0",
    "@angular/material-date-fns-adapter": "^21.0.0",
    "@angular/platform-browser": "^21.0.0",
    "@angular/platform-server": "^21.0.0",
    "@angular/router": "^21.0.0",
    "@angular/ssr": "^21.0.0",
    "@fontsource/nunito": "^5.2.7",
    "@fontsource/source-code-pro": "^5.2.7",
    "@highlightjs/cdn-assets": "^11.11.1",
    "@material-symbols/font-500": "^0.39.4",
    "date-fns": "^4.1.0",
    "express": "^5.1.0",
    "highlight.js": "^11.11.1",
    "marked": "^17.0.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@angular/build": "^21.0.0",
    "@angular/cli": "^21.0.0",
    "@angular/compiler-cli": "^21.0.0",
    "@types/express": "^5.0.1",
    "@types/node": "^20.17.19",
    "jsdom": "^27.1.0",
    "ng-packagr": "^21.0.0",
    "prettier": "^3.6.2",
    "prettier-plugin-organize-imports": "^4.3.0",
    "typescript": "~5.9.2",
    "vitest": "^4.0.8"
  }
}
